import unittest


def add(x, y):
    # add function
    return x + y


def subtract(x, y):
    # subtract function
    return x - y


def multiply(x, y):
    # multiply function
    return x * y


def divide(x, y):
    # divide function
    if y == 0:
        raise ValueError('Can not divide by zero!')
    return x / y


class TestCalc(unittest.TestCase):

    def test_add(self):
        result = add(10, 5)
        self.assertEqual(result, 15)


print(add(10, 5))
